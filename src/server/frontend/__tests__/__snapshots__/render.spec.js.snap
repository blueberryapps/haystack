exports[`test render 1`] = `"<!DOCTYPE html><html lang=\"en\"><head><meta charset=\"utf-8\"/><meta content=\"width=device-width, initial-scale=1, shrink-to-fit=no, user-scalable=no\" name=\"viewport\"/><meta content=\"ie=edge\" http-equiv=\"x-ua-compatible\"/><title data-react-helmet=\"true\"></title></head><body><div id=\"app\"><div id=\"app\"><div data-reactroot=\"\" data-reactid=\"1\" data-react-checksum=\"1998851930\"></div></div></div><script type=\"text/javascript\">window.REDUX_STATE={\"contactForm\":{\"formSendingStatus\":\"\"},\"onionForm\":{\"fields\":{}},\"router\":{\"location\":null},\"translate\":{\"messages\":{\"defaultEn\":{\"contact\":{\"heading\":{\"small\":\"Feel free to say hello!\",\"text_1\":\"Contact\",\"text_2\":\"us\"},\"claim\":{\"text_1\":\"or let’s\",\"text_2\":\"have a coffee\",\"text_3\":\"together\"},\"bar\":{\"follow\":\"and follow us\",\"mail\":\"Mail\",\"phone\":\"(855) 725-8277\",\"skype\":\"Hello blueberry_io\"},\"form\":{\"message\":{\"success\":\"Message sent, Thank you.\"},\"submit\":\"Send your message\"},\"links\":{\"facebook\":\"https:\\u002F\\u002Fwww.facebook.com\\u002FBlueberry.cz\",\"linkedIn\":\"https:\\u002F\\u002Fwww.linkedin.com\\u002Fcompany\\u002F897163\",\"mail\":\"hi@blueberry.io\",\"phone\":\"(855) 725-8277\",\"skype\":\"blueberry_io?call\",\"twitter\":\"http:\\u002F\\u002Ftwitter.com\\u002Fblueberry_cz\"}},\"form\":{\"cv\":{\"label\":\"Your CV (optional)\",\"placeholder\":\"Your CV drops here\"},\"email\":{\"label\":\"Your email\",\"placeholder\":\"example@example.com\"},\"message\":{\"label\":\"Your message\",\"placeholder\":\"Your message...\"},\"name\":{\"label\":\"Your name\",\"placeholder\":\"Your name\"}},\"getInTouch\":\"Lets make something great together\",\"meet\":{\"seeTeam\":\"See our team\",\"heading\":{\"small\":\"Meet Blueberry\'s\",\"text_1\":\"superb\",\"text_2\":\"\\u003Cbr\\u003Ededicated \\u003Cbr\\u003E team\"},\"reactbulb\":{\"heading\":\"40 skilled developers\",\"text\":\"We know our stuff. Our devs give advanced workshops on the technologies we work with.\"},\"years7\":{\"heading\":\"7 years running\",\"text\":\"Our company’s been operating for more than 7 years.\"},\"local\":{\"heading\":\"Local\",\"text\":\"We have offices in the United States and in Prague\"},\"earth\":{\"heading\":\"Reachable\",\"text\":\"We meet with you in-person to fully understand your project and deliver the web or mobile applications that best suit your needs.\"}},\"number\":{\"currency\":{\"format\":{\"delimiter\":\".\",\"format\":\"%n %u\",\"precision\":0,\"separator\":\",\",\"unit\":\"kr.\"}},\"format\":{\"delimiter\":\".\",\"precision\":2,\"separator\":\",\"},\"percentage\":{\"format\":{\"delimiter\":\"\",\"format\":\"%n %\"}},\"precision\":{\"format\":{\"delimiter\":\"\"}}},\"errors\":{\"back\":\"Go back to homepage\",\"page404\":{\"blueTitle\":\"doesn\'t exist\",\"blackTitle\":\"Page\",\"subtitle\":\"Sorry for that\"},\"page500\":{\"blueTitle\":\"bad\",\"blackTitle\":\"Something\",\"subtitle\":\"just happened\"}},\"intro\":{\"button\":\"Get in touch\",\"title\":\"See how we work\",\"headingWeb\":\"web \",\"headingApp\":\"mobile apps\",\"headingSmall\":\"We help companies transform ideas into exceptional\"},\"date\":{\"format\":\"D\\u002FM YYYY\",\"days\":\"{count} {count, plural, one {day} other {days}}\",\"months\":\"{count} {count, plural, one {month} other {months}}\",\"month\":{\"short\":\"mo.\"}},\"homepage\":{\"form\":{\"heading\":{\"text_1\":\"Let’s start\",\"text_2\":\"working\",\"text_3\":\"together.\"}},\"heading\":\"Home\",\"projects\":{\"heading\":{\"small\":\"Explore uniqueness of\",\"text_1\":\"our\",\"text_2\":\"projects\"}},\"work\":{\"heading\":{\"small\":\"Explore uniqueness of\",\"text_1\":\"our\",\"text_2\":\"work\"}}},\"how\":{\"heading\":{\"text_1\":\"work\",\"text_2\":\"together\",\"small\":\"How we can\"},\"product\":{\"heading\":\"Whole product\",\"text\":\"We can work with you to develop your idea into a full-fledged mobile or web application or a ready-to-use backend solution.\"},\"boost\":{\"heading\":\"Team boost\",\"text\":\"We can work as one of your teams or join an existing team. We can help you with JavaScript or Ruby development.\"},\"kickoff\":{\"heading\":\"Project kick-off\",\"text\":\"We can help you get off the ground faster by offering training, guidance in technological decisions, and setting up core workflows.\"},\"research\":{\"heading\":\"And more\",\"text\":\"We offer training, audits, and more. Get in touch to see if we can suit your need.\"}},\"work\":{\"heading\":{\"small\":\"For clients and for blueberry\",\"text_1\":\"Our\",\"text_2\":\"work\"},\"colaborate\":\"Lets start working together\",\"card\":{\"bluekit\":{\"heading\":\"BlueKit\",\"intro\":\"BlueKit is our tool for automatically generating a component library from React component files. It offers a visual overview of all components and their props, which is a need for both developers and designers alike. Most importantly, it’s open-source, so anyone can use it.\",\"label\":\"Elements library for developers\"},\"elevate\":{\"heading\":\"ElevateHD\",\"intro\":\"While we were first called on only to create a minimum viable product for elevateHD, our collaboration grew deeper as we helped find and apply a viable business model, brought the product to the web, and ultimately became responsible for the entire product.\",\"label\":\"iOS and web application\",\"link\":\"Explore ElevateHD\"},\"itsounds\":{\"heading\":\"itSOUNDS\",\"intro\":\"With itSOUNDS, our task was to bring the company’s online presence in line with its brand identity. Our web redesign not only matched the brand’s high-end nature, but was able to evoke the mysterious charm of listening to music with considered animations and interaction tweaks.\",\"label\":\"Premium design speakers\",\"link\":\"Explore itSOUNDS\"},\"meatandbones\":{\"heading\":\"Meat & Bones\",\"intro\":\"Meat & Bones has a special place in our hearts. We’ve been there since its inception, figuring out whether there is enough demand for such a service, and made it grow into the service it is today, using our design, development, and marketing expertise to create something delightful.\",\"label\":\"Dog food service\",\"link\":\"Explore Meat & Bones\"},\"ppcbee\":{\"heading\":\"PPCBee\",\"intro\":\"PPC Bee is a tool for the automation of pay-per-click product campaigns for online stores. Started outside Blueberry as a set of scripts, we grew it into a capable system with a strong team behind it and a user-centric focus.\",\"label\":\"PPC tool for online stores\",\"link\":\"Explore PPC Bee\"},\"tcox\":{\"heading\":\"TCOX\",\"intro\":\"The complete redesign of the Tayllor & Cox website was a thorough yet rewarding task for us. We’re quite proud of the meticulously optimized buying process and the more effective homepage, but also of the custom-made CMS we created in the process, making it easy and efficient to administer the complex website.\",\"label\":\"Accredited Trainings\"},\"zaplo\":{\"heading\":\"Zaplo\",\"intro\":\"With the Zaplo.dk project, we were tasked with figuring out how to make software releases faster for 4finance, a leading global digital consumer finance company. We not only showed how work could be done more efficiently, but also how to communicate effectively and structure code better, setting an example for their other projects to follow.\",\"label\":\"Global consumer finance\",\"link\":\"Explore Zaplo.dk\"}},\"detail\":{\"elevate\":{\"heading\":\"elevateHD\",\"client\":{\"app\":\"iOS app\",\"rails\":\"Ruby on Rails\",\"angular\":\"AngularJS\",\"heading\":\"About client\",\"subheading\":\"Collaboration\",\"ux\":\"UX design\",\"contentful\":\"Contentful CMS\",\"chef\":\"Chef\",\"es6\":\"ECMAScript 2015\",\"text\":\"elevateHD — Elevate Home Design allows users to quickly and cost-effectively design their own homes. It’s available on both iOS and the web. The service is offered to the end-customer segment, but also exists as a licenced solution for B2B to improve online conversion rate and enhance the shopping experience for the customer.\",\"rubymotion\":\"RubyMotion\",\"translation\":\"Translation server\",\"frontend\":\"Web app front-end\",\"strategy\":\"Digital strategy\",\"backend\":\"Web app back-end\",\"nightwatch\":\"Nightwatch.js\",\"going\":\"2 years and going\",\"design\":\"Visual design\",\"react\":\"React\"},\"live\":{\"text\":\"Check out live version\",\"link\":\"Visit elevateHD website\"},\"technology\":\"Technology Highlights\",\"url\":\"http:\\u002F\\u002Fwww.elevatehomedesign.com\",\"perex\":\"elevateHD is a service that provides affordable home design to its users. While we were first called on only to create a minimum viable product for elevateHD, our collaboration grew deeper as we helped identify a viable business model, brought the product to the web, and ultimately became responsible for the entire product.\",\"hire\":\"Hire Us\",\"project\":{\"heading\":\"The Challenge\",\"text\":\"Our client came to us with a vision — a fast and affordable interior design service without the inefficiencies of in-person consultations. The client asked us, the developers behind the project, to validate his business idea.\",\"subheading\":\"Our Solution\",\"subtext1\":\"To test the client’s vision, we created a solid MVP iOS app. Results after testing showed a need for a restructuring of the business concept, and our cooperation with the client shifted to a higher level.\",\"subtext2\":\"Our team helped shift focus from a B2C-only solution to both a B2C and B2B platform and brought the app to the web to reach a wider audience. Blueberry is now responsible for product management of the whole platform and drives innovation behind the product.\"},\"link\":\"Visit elevateHD website\"},\"itsounds\":{\"heading\":\"itSOUNDS\",\"client\":{\"head\":\"About\\u003Cbr\\u003Ethe client\",\"saas\":\"Saas\",\"rails\":\"Ruby on Rails\",\"subheading\":\"Collaboration\",\"text\":\"itSOUNDS creates high-end design speakers. The brand insists on the highest quality listening experience and displays love for a colorful music experience, special moments, and a feeling of connection. They allow no compromise in construction quality and product design behind such values.\",\"invision\":\"inVision\",\"frontend\":\"Web development\",\"strategy\":\"Digital strategy\",\"sandglass\":\"330 hours\",\"design\":\"Visual design\"},\"live\":{\"text\":\"Check out live version\",\"link\":\"Visit itSOUNDS website\"},\"technology\":\"Technology highlights\",\"url\":\"\\u002F\\u002Fwww.itsounds.com\\u002F\",\"perex\":\"itSOUNDS is a maker of high-end speakers. The task they called on us for was bringing the company’s online presence in line with its brand identity. Our web redesign not only matched the brand’s high-end nature, but was able to evoke the mysterious charm of listening to music with considered animations and interaction tweaks.\",\"hire\":\"Hire us\",\"project\":{\"heading\":\"The challenge\",\"subheading\":\"Our solution\",\"text1\":\"The brief for the project was to redesign itSOUNDS’s old website, which was of a low standard and simply didn’t represent the image of a luxury brand. Our mission was to create a new one reflecting not only the quality and technology of its high-end products, but also its brand personality and spirit.\",\"text2\":\"We started with a deep understanding of the itSOUNDS brand, with thorough input from the owner. We extended their brand identity to be able to communicate the spirit and the essence of itSOUNDS, both visually and in writing. The web development phase followed, resulting in a responsive website focused on imagery, emotion, and the secrets connected with the brand.\"},\"link\":\"Visit itSOUNDS website\"},\"ppcbee\":{\"heading\":\"PPC Bee\",\"circles\":{\"elasticsearch\":\"Elasticsearch\",\"postgresql\":\"PostgreSQL\",\"rails\":\"Ruby on rails\",\"redis\":\"Redis\",\"ruby\":\"Ruby\"},\"client\":{\"head\":\"PPC Bee’s\\u003Cbr\\u003ERoots\",\"text\":\"Managing PPC campaigns at his former job made one of our key programmers, Ondřej Bartas, realize that some of the work he was doing could be automated. He initially wrote a script for himself, but soon realized its potential and joined up with a friend to start selling it. They worked in their spare time, rearchitecturing the product, adding a front-end, and gaining multiple clients. However, it was hard to use and work in two was proving to be tiresome.\",\"subheading\":\"Who’s using PPC Bee\"},\"live\":{\"text\":\"Check out live version\",\"url\":\"http:\\u002F\\u002Fppcbee.cz\",\"link\":\"Visit PPC Bee \"},\"technology\":\"Technology highlights\",\"url\":\"http:\\u002F\\u002Fppcbee.cz\",\"perex\":\"PPC Bee is a tool for the automation of pay-per-click product campaigns for online stores. Started outside Blueberry as a set of scripts, we grew it into a capable system with a strong team behind it and a user-centric focus.\",\"hire\":\"Hire Us\",\"project\":{\"heading\":\"Our Solution\",\"text1\":\"When Bartas joined Blueberry, the company’s leadership decided that they want the project to thrive, investing heavily in the project. Blueberry provided more development power, designers, and marketing specialists. Most importantly, PPC Bee was assigned a product manager to manage the project, letting Bartas act as a supervisor and devote time to what he enjoys most.\",\"text2\":\"With the goal to present the project at TechCrunch Disrupt 2015, development was accelerated and user experience became a major focus. After a complete redesign of the core, PPC Bee became not only more usable, but also far more scalable, paving the way for the product to grow much faster. We switched PPC Bee to a SaaS business model, giving us a more stable source of funding and allowing us to focus on our clients’ needs. Today, PPC Bee maintains its focus on user experience and has a clientele that operates all around Europe.\"},\"link\":\"Visit PPc Bee\"},\"zaplo\":{\"heading\":\"Zaplo.dk\",\"client\":{\"head\":\"The Client\",\"subheading\":\"Collaboration\",\"contentful\":\"Contentful CMS\",\"chef\":\"Chef\",\"es6\":\"ECMAScript 2015\",\"text\":\"4Finance is a global leader in digital consumer finance, using cutting-edge technology and data to offer fast and convenient loans to customers around the world.\",\"translation\":\"Translation server\",\"frontend\":\"Web app front-end\",\"nightwatch\":\"Nightwatch.js\",\"going\":\"4 months\",\"development\":\"Change development\",\"react\":\"React\"},\"live\":{\"text\":\"Check out live version\",\"link\":\"Visit Zaplo.dk website\"},\"technology\":\"Technology Highlights\",\"url\":\"http:\\u002F\\u002Fwww.zaplo.dk\",\"perex\":\"Zaplo.dk is a web application we made for 4finance, a leading consumer finance company. It allows people to get a loan quickly. In creating it, we not only showed how work could be completed more efficiently, but also how to communicate effectively and structure code better, setting an example for other 4finance projects to follow.\",\"hire\":\"Hire Us\",\"project\":{\"heading\":\"The Challenge\",\"text1_1\":\"The client came to us to solve a recurring problem: the development process behind software releases was taking too long. We were tasked with developing a solution as a proof-of-concept of our new approach.\",\"text1_2\":\"We conducted an in-depth analysis of existing 4finance projects and concluded that two main factors were to blame: it was taking too long to make the front-end for websites and it was hard to find developers experienced in the technological framework that was being used.\",\"text2_1\":\"To speed up front-end development, we worked alongside the 4finance team to deeply understand the problem, the product, and the people. This investigation lead us to reorganize the multidisciplinary development teams, which improved the communication between back-end and front-end developers, thus reducing the time needed to complete the process.\",\"text2_2\":\"Taking into account the need for more skilled developers, we researched and selected React.js as the best platform. Besides bringing in more development power, React.js allowed us to reuse more code, significantly reducing development time.\",\"text2_3\":\"The project was a resounding success. Zaplo.dk demonstrated how work could be completed more efficiently, created better communication, and improved code structure — setting a precedent for their future projects.\",\"subheading\":\"Our Solution\"},\"link\":\"Visit Zaplo.dk site\"},\"meatandbones\":{\"heading\":\"Meat & Bones\",\"client\":{\"rails\":\"Ruby on Rails\",\"heading\":\"About client\",\"subheading\":\"Collaboration\",\"ux\":\"UX design\",\"marketing\":\"Marketing strategy\",\"stripe\":\"Stripe\",\"text\":\"Meat & Bones provides raw dog food as a subscription service. This company has a special place in our hearts. This service was established in the San Francisco Bay Area, shipping raw food to California, Oregon, and Washington.\",\"frontend\":\"Web app front-end\",\"strategy\":\"Digital strategy\",\"backend\":\"Web app back-end\",\"going\":\"1 years and going\",\"design\":\"Visual design\",\"react\":\"React\"},\"live\":{\"text\":\"Check out live version\",\"link\":\"Visit Meat & Bones website\"},\"technology\":\"Technology Highlights\",\"url\":\"http:\\u002F\\u002Fwww.meatandbones.com\",\"perex\":\"Meat & Bones provides raw dog food as a subscription service. This company has a special place in our hearts. We’ve been there since its inception, figuring out whether there is enough demand for such a service, and helped it grow into the service it is today, using our design, development, and marketing expertise to create a delightful, healthy, and easy way to feed pets.\",\"hire\":\"Hire Us\",\"project\":{\"heading\":\"The Challenge\",\"text\":\"The client came to us to gain insight into customer interest for a raw dog food subscription service and asked us to build an online platform focused on the presentation and the delivery of the food.\",\"subheading\":\"The Solution\",\"subtext1\":\"A multidisciplinary team was assigned to the project. In the first phase, we created a landing page and an ad campaign, the goal of which was to gauge the general interest of customers\",\"subtext2\":\"After confirming interest, we began the service development process. We researched competitors and interviewed potential customers as a basis, and furthered this with mockups and the release of an MVP. Our first version, which only allowed pre-orders, was used to test and promote the service as well as to gain data for further iteration\",\"subtext3\":\"Besides developing the platform, we’ve also been busy marketing it, and we’re still creating new marketing campaigns as we flesh out the website. We’re helping Meat & Bones create a great service for dog-lovers who care about proper nutrition and a healthy life.\"},\"link\":\"Visit Meat & Bones website\"},\"tcox\":{\"heading\":\"TaylorCox\",\"perex\":\"The complete redesign of the Tayllor & Cox website was a thorough yet rewarding task for us. We’re quite proud of the meticulously optimized buying process and the more effective homepage, but also of the custom-made CMS we createdin the process, making it easy and efficient to administer the complex website.\",\"link\":\"Visit TCOX website\",\"live\":{\"text\":\"Check out live version\",\"link\":\"Visit TCOX site\"},\"client\":{\"heading\":\"About the client\",\"subheading\":\"Collaboration\",\"goingText\":\"We know our stuff. Our devs give advanced workshops on the technologies we work with.\",\"developmentText\":\"We know our stuff. Our devs give advanced workshops on the technologies we work with.\",\"agileText\":\"We know our stuff. Our devs give advanced workshops on the technologies we work with.\",\"text\":\"Tayllor & Cox is an Accredited Training Organisation for PRINCE 2, MOP, ITIL, SCRUM, MSP, ISMS and more accredited by Axelos. At the same time, it works as a Registered Certification Body for professional services, audits and career development. It is eminent in the Czech Republic, and all around Europe.\",\"going\":\"4 months of development\",\"development\":\"Change development\",\"agile\":\"Agile development\"},\"project\":{\"heading\":\"Challange\",\"text\":\"Tayllor & Cox set a clear goal to create a number one website, representing its dominance and excellence in the market. The client faced many challenges with its robust and complex web presentation. There was no mobile optimization, it was difficult and time consuming to load web content, there was a low rate of online conversions, and the site did not reflect the profile of the most frequent customers enough. Above all, the client wanted to improve the level of automation in the buying process online -- i.e. reduce assistance needed by customer support in the order form process.\",\"subheading\":\"Solution\",\"subtext1\":\"The project started with analysis of the website, especially focused on the buying process and its weak spots. We also gathered information about customer profiles and did competitor research. After this phase, we started with prototyping the website with respect to key findings about the needs of customers. Findings were reflected in the homepage design to bring important touchpoints to the front. and in the right order.\",\"subtext2\":\"Special care was taken with regards to the buying process, where we got through many rounds of iteration and user testing. Innovated backend includes custom made CMS with improved UX and functionality enabling anybody to work with it, ensuring consistency of content design on the website. The backend is connected to and automated email templates system. In general, we have delivered an innovative website with improved usability and higher online conversion potential and the backend easy to maintain, with automated buying process features.\"},\"hire\":\"Hire Us\"}},\"more\":\"Show all our projects\"},\"twitter\":{\"heading\":\"Life in our team\",\"loading\":\"Loading Blueberry tweets\",\"provider\":\"via Twitter\"},\"history\":{\"heading\":{\"small\":\"We have rich basics\",\"text_1\":\"Our\",\"text_2\":\"history\"}},\"footer\":{\"copyright\":{\"by\":\"This page was handcrafted with\",\"company\":\"2016 Blueberry.io\",\"where\":\"in United States\"},\"section\":{\"company\":{\"heading\":\"Company\",\"items\":{\"work\":{\"heading\":\"Our work\"},\"team\":{\"heading\":\"Our team\"},\"contacts\":{\"heading\":\"Contacts\"}}},\"social\":{\"heading\":\"Social\",\"items\":{\"facebook\":{\"heading\":\"Facebook\"},\"twitter\":{\"heading\":\"Twitter\"},\"linkedIn\":{\"heading\":\"LinkedIn\"}}},\"contact\":{\"heading\":\"Contact us\",\"items\":{\"prg\":{\"heading\":\"Křížová 2598\\u002F4\\u003Cbr\\u002F\\u003EPrague, Czech republic\"},\"sf\":{\"heading\":\"We Work\\u003Cbr\\u002F\\u003E220 NW 8th Ave\\u003Cbr\\u002F\\u003EPortland, OR 97205\"},\"srb\":{\"heading\":\"Nikole Pašića 29\\u003Cbr\\u002F\\u003ENovi Sad, Serbia\"}}}}},\"team\":{\"heading\":{\"text_1\":\"Our\",\"text_2\":\"team\",\"small\":\"We are passionate humans\"},\"heading2\":{\"text_1\":\"True perfection\",\"text_2\":\"lies on people you\",\"text_3\":\"workin with\"},\"heading3\":{\"text_1\":\"We create\",\"text_2\":\"a stunning web sites\",\"text_3\":\"and fresh design\"},\"contactHeading\":{\"small\":\"Send us a CV and we will see\",\"text_1\":\"Want to be\",\"text_2\":\"part of our\",\"text_3\":\"team?\"},\"group\":{\"applications\":\"Applications\",\"leadership\":\"Leadership\",\"bussiness_development\":\"Business Development\",\"development\":\"Development\",\"marketing\":\"Marketing\",\"hr_operations\":\"HR & Operations\",\"design\":\"Design\"}},\"services\":{\"heading\":\"Services\",\"intro\":{\"heading\":{\"small\":\"create digital worlds\",\"text_1\":\"How\",\"text_2\":\"we\"},\"text\":\"create digital worlds\"},\"weDevelop\":{\"heading\":{\"text_1\":\"We\",\"text_2\":\"develop\"},\"backend\":{\"label\":\"Full-service agency\",\"heading\":\"Back-ends\",\"text\":\"We make both entire apps, complete with back-end and front-end, and standalone back-end - then non-UI parts of an app. We\'ve found Ruby on Rails, Node.js, and newly Elxir to work well for us, but we aren\'t strangers to other technologies.\"},\"frontend\":{\"label\":\"Developing with love\",\"heading\":\"Front-ends\",\"text\":\"If you just need a different UI, we can make a \\u003Cb\\u003Ewebsite or a native mobile app\\u003C\\u002Fb\\u003E on top of your existing back-end. React and \\u003Ca style=\\\"color: #00A7D7; text-decoration: underline\\\"\\u003EReact Native\\u003C\\u002Fa\\u003E are our bread-and-butter, but we\'ll use whatever is most suitable for your project.\",\"button\":\"Read more about our front-ends\"}},\"weDesign\":{\"heading\":\"We design\",\"interaction\":{\"heading\":\"Interaction design\",\"label\":\"Pixel-perfect perfectionists\",\"mainheading\":\"We design\",\"text\":\"We\'re a design driven company and put user experience above everything. Our design is informed by research, tested using prototypes, and iteratively improved based on analytics and user testing.\"},\"graphics\":{\"heading\":\"Graphic design\",\"text\":\"In terms of graphics design, we do a lot — from UI to branding to marketing materials. Designing your brand from scratch is not a problem for us.\",\"button\":\"See how we design\"}},\"digitalMarketing\":{\"button\":\"See how we approach marketing\",\"heading\":{\"small\":\"We love\",\"text_1\":\"digital\",\"text_2\":\"marketing\"},\"label\":\"Invetive solutions\",\"text\":\"We can make you a complex marketing campaign that\'s custom-fit for your product. We\'ll track your campaign\'s progress and optimize for success.\"}},\"locations\":{\"heading\":{\"small\":\"Explore our offices and places\",\"text_1\":\"where\",\"text_2\":\"we work\"},\"office\":\"Our office in\",\"prg\":{\"heading\":\"Prague\",\"city\":\"Prague, Czech republic\",\"street\":\"Křížová 4, Praha 5\",\"zip\":\"150 00\"},\"sf\":{\"heading\":\"Portland\",\"city\":\"Oregon, USA\",\"street\":\"220 NW 8th Ave\",\"zip\":\"Portland, OR 97205\"},\"srb\":{\"heading\":\"Novi Sad\",\"city\":\"Novi Sad, Serbia\",\"street\":\"Nikole Pašića 29\",\"zip\":\"21000\"}},\"career\":{\"heading\":{\"small\":\"Want to work\",\"text_1\":\"With\",\"text_2\":\"us?\"},\"form\":{\"heading_part1\":\"Studying\",\"heading_part2\":\"and looking for\",\"heading_part3\":\"an internship?\",\"heading_small\":\"Send a message, we\'ll get in touch\"},\"positions\":{\"creatives\":\"Kreativci\",\"czechOnly\":\"(Czech&nbsp;only)\",\"development\":\"Styvitelé\",\"heading\":{\"text_1\":\"We want\",\"text_2\":\"to hire\"},\"operatives\":\"Operativci\"},\"banner\":{\"heading\":\"We\'re real people with a passion and a love for the things we do\"},\"about\":{\"heading\":{\"text_1\":\"About\",\"text_2\":\"us\"},\"text1\":\"We\'re creating digital worlds and enjoying it. We develop successful mobile and web applications, create websites with captivating graphics, think up and manage effective online marketing campaigns that prosper. The portfolio of our services covers everything needed for success in an age of internet dominance. Our domain is hassle-free collaboration that leads to the realization of projects both large and small.\"},\"culture\":{\"heading\":{\"text_1\":\"Our\",\"text_2\":\"culture\"},\"text1\":\"We\'re a group of friends with a common drive. Creating perfect digital works.\",\"text2\":\"Are you a Ruby fan wondering what React can do? We\'ll send you to a training, make sure you\'re mentored by older colleagues. If you have drive, we\'ll be the last ones standing in your way.\",\"text3\":\"Our hierarchy is simple. We\'re a team, we pull together. Have a problem? Your boss is right within reach, ready to discuss and solve your problems.\",\"text4\":\"A pretty office isn\'t everything. Here you have a number of options that will brighten up your work day. Do you think better when standing? That\'s what our standing desks are for! And we have more goodies where that came from.\",\"subheading1\":\"A friendly atmosphere and casual environment\",\"subheading2\":\"Opportunity for career growth\",\"subheading3\":\"A human approach from bosses\",\"subheading4\":\"Office environment\"},\"detail\":{\"culture\":{\"heading\":{\"text_1\":\"Our\",\"text_2\":\"culture\"},\"text\":\"Blueberry is a digital full-service agency that consists of people excited about the work they do. We develop interesting web applications, create captivating presentational websites, and think up effective online marketing campaigns. Aside from people, we have a few dogs running around our office and there’s even an aquarium with shrimp here. In the morning, ping pong and foosball help wake us up.\"},\"expect\":{\"text_1\":\"What to\",\"text_2\":\"expect\"},\"form\":{\"heading\":{\"text_1\":\"Do you want\",\"text_2\":\"to join us?\",\"text_3\":\"Write to us!\"}},\"working\":{\"small\":\"Projects you will be\",\"text_1\":\"working\",\"text_2\":\"on\"},\"team\":{\"heading\":{\"small\":\"People you will be\",\"text_1\":\"working\",\"text_2\":\"with\"}},\"video\":{\"heading\":{\"small\":\"What we are\",\"text_1\":\"dealing\",\"text_2\":\"with\"},\"text\":\"We hire people, not roles. If you feel like your knowledge and experience don\'t completely match up with our requirements, that\'s okay. Write to us anyway. Maybe you\'re a person that our team needs, and we just don\'t know it yet. Let us know about yourself — you have that opportunity!\"}},\"title\":\"Want to work with us?\",\"benefits\":{\"heading\":{\"text_1\":\"Company\",\"text_2\":\"benefits\"},\"text1\":\"You won\'t get annoyed by slow tech when working.\",\"text2\":\"Your animal friend is welcome here!\",\"text3\":\"An apple a day keeps the doctor away! Here, your needed vitamins and nutrients are right within reach.\",\"text4\":\"Helping the less fortunate isn\'t just an empty phrase. You get to choose who we send our money to. Helping does make a difference.\",\"text5\":\"A pingpong table, foosball, TRX, sofas, and comfy pillows. We take care to make you feel good and rested.\",\"text6\":\"Sick child? Or maybe sick fish? When you need to, you can work from home. We\'re a digital company, we can communicate online.\",\"text7\":\"If you get a sweet tooth, you can get rid of your munchies for five crowns in our charming snack area.\",\"text8\":\"Friendship and good relationships are our biggest asset. Live through some real adventures with us on a vacation in Italy or playing paintball in Prague.\",\"text9\":\"You keep trying to grow and enjoy learning? We\'ll give you enough space for that.\",\"subheading1\":\"A company MacBook for devs and designers\",\"subheading2\":\"Pet Friendly office\",\"subheading3\":\"Fresh fruit\",\"subheading4\":\"We\'ll match 1% of your salary\",\"subheading5\":\"Play room\\u002Frelax room\",\"subheading6\":\"Home office\",\"subheading7\":\"Sweets and snacks\",\"subheading8\":\"Regular teambuilding events\",\"subheading9\":\"Self-learning\"}},\"menu\":{\"career\":\"Career\",\"contact\":\"Contact\",\"history\":\"History\",\"home\":\"Home\",\"team\":\"Team\",\"work\":\"Work\",\"services\":\"Services\"},\"workflow\":{\"heading\":{\"small\":\"We are proud of\",\"text_1\":\" how\",\"text_2\":\"we \\u003Cbr\\u003E work\"},\"brief\":{\"heading\":\"Brief\",\"text\":\"We are open to discussing your business. The first meeting helps both sides understand each other and set the right expectations.\"},\"strategy\":{\"heading\":\"Digital Strategy Consultation\",\"text\":\"Before we start designing your product, it is extremely important for us to get familiar with your business idea. Furthermore, we will help you analyze your business environment and define your target audience. Based on the needs of your customers, we will propose an appropriate digital strategy that will fit your business. Before we move on, we will test our assumptions.\"},\"ux\":{\"heading\":\"UX Design\",\"text\":\"User-centered design is our path to digital excellence. Combining research, creative ideas and prototyping, we design a product based on real user insights and with clear KPIs.\"},\"research\":{\"heading\":\"Research\",\"text\":\"No business plan survives first contact with customers. Understanding real people and their needs is at the core of what good design is built on. That is why basic assumptions need to be verified at the beginning. However, collecting continuous feedback is key for further improvement and allows us to respond flexibly to the users\' needs. We see research as a process rather than a stage.\"},\"idea\":{\"heading\":\"Ideation & Exploration\",\"text\":\"Based on different types of research, we approach design challenges with the whole team from multiple perspectives. This helps us map out user behavior and understand it.\"},\"prototype\":{\"heading\":\"Prototype & Visual Design\",\"text\":\"We start to sketch with pen and paper to capture our initial thoughts. We then convert these wireframes into a clickable prototype using various digital design tools. The prototype is used to test our ideas and assumptions in the real world. And finally, we prepare high fidelity visual design.\"},\"agile\":{\"heading\":\"Agile Development\",\"text1\":\"We follow the Scrum agile development methodology. We work in short iterations called sprints. After each sprint, we deliver a working product with new features and present it to you.\",\"text2\":\"You are always in control of the development process and can make changes to the product as we go, and ultimately decide when the right time to launch is. While we build, we gather feedback from  customers and make sure that we\'re developing the right product.\"}}},\"en\":{\"en\":{\"contact\":{\"bar\":{\"follow\":\"and follow us\",\"mail\":\"Mail\",\"mail_address\":\"hi@blueberry.io\",\"phone\":\"(855) 725-8277\",\"phone_number\":\"(855) 725-8277\",\"skype\":\"Skype us\",\"skype_text\":\"Skype us\"},\"claim\":\"or let\'s have a coffee together\",\"form\":{\"label\":{\"cv\":\"Your CV (optional)\",\"email\":\"Your email\",\"message\":\"Your message\",\"name\":\"Your name\"},\"message\":{\"success\":\"Message sent, Thank you.\"},\"placeholder\":{\"cv\":\"Your CV drops here\",\"email\":\"example@example.com\",\"message\":\"Your message...\"},\"submit\":\"Send your message\"},\"heading\":\"Contact Us\",\"links\":{\"facebook\":\"https:\\u002F\\u002Fwww.facebook.com\\u002FBlueberry.cz\",\"linkedIn\":\"https:\\u002F\\u002Fwww.linkedin.com\\u002Fcompany\\u002F897163\",\"mail\":\"hi@blueberry.io\",\"phone\":\"0018557258277\",\"skype\":\"support@blueberryapps.com?call\",\"twitter\":\"http:\\u002F\\u002Ftwitter.com\\u002Fblueberry_cz\"}},\"getInTouch\":\"Lets make something great together\",\"meet\":{\"earth\":{\"heading\":\"Reachable\",\"text\":\"We meet with you in-person to fully understand your project and deliver the web or mobile applications that best suit your needs.\"},\"heading\":\"Meet Blueberry\'s dedicated team\",\"local\":{\"heading\":\"Local\",\"text\":\"We have offices in the United States and in Prague\"},\"reactbulb\":{\"heading\":\"40 skilled developers\",\"text\":\"We know our stuff. Our devs give advanced workshops on the technologies we work with.\"},\"years7\":{\"heading\":\"7 years running\",\"text\":\"Our company’s been operating for more than 7 years.\"}},\"error\":{\"back\":\"Go back to homepage\",\"page404\":{\"blackTitle\":\"doesn\'t exist\",\"blueTitle\":\"Page\",\"subtitle\":\"Sorry for that\"},\"page500\":{\"blackTitle\":\"Something\",\"blueTitle\":\"bad\",\"subtitle\":\"just happened\"}},\"errors\":{\"back\":\"Go back to homepage\",\"page404\":{\"blackTitle\":\"Page\",\"blueTitle\":\"doesn\'t exist\",\"subtitle\":\"Sorry for that\"},\"page500\":{\"blackTitle\":\"Something\",\"blueTitle\":\"bad\",\"subtitle\":\"just happened\"}},\"intro\":{\"button\":\"Get in touch\",\"heading\":\"Web + mobile Apps\",\"title\":\"See how we work\"},\"homepage\":{\"heading\":\"Home\",\"projects\":{\"heading\":\"Explore uniqueness of our projects\"},\"work\":{\"heading\":\"Explore uniqueness of our work\"}},\"how\":{\"boost\":{\"heading\":\"Team boost\",\"text\":\"We can work as one of your teams \\u0003or join an existing team. We can help you with JavaScript or Ruby \\u0003development.\"},\"heading\":\"How we can work together\",\"kickoff\":{\"heading\":\"Project kick-off\",\"text\":\"We can help you get off the ground faster by offering training, guidance in technological \\u0003decisions, and setting up core workflows.\"},\"product\":{\"heading\":\"Whole product\",\"text\":\"We can work with you to develop your idea into a full-fledged mobile or web application or \\u0003a ready-to-use backend solution.\"},\"research\":{\"heading\":\"And more\",\"text\":\"We offer training, audits, and more. Get in touch to see if we can suit \\u0003your need.\"}},\"work\":{\"bluekit\":{\"heading\":\"BlueKit\",\"intro\":\"BlueKit is our tool for automatically generating a component library from React component files. It offers a visual overview of all components and their props, which is a need for both developers and designers alike. Most importantly, it’s open-source, so anyone can use it.\",\"label\":\"Elements library for developers\"},\"card\":{\"bluekit\":{\"heading\":\"BlueKit\",\"intro\":\"BlueKit is our tool for automatically generating a component library from React component files. It offers a visual overview of all components and their props, which is a need for both developers and designers alike. Most importantly, it’s open-source, so anyone can use it.\",\"label\":\"Elements library for developers\"},\"elevate\":{\"heading\":\"ElevateHD\",\"intro\":\"While we were first called on only to create a minimum viable product \\u0003for elevateHD, our collaboration grew deeper as we helped find and \\u0003apply a viable business model, brought the product to the web, and \\u0003ultimately became responsible for the entire product.\",\"label\":\"iOS and web application\",\"link\":\"Explore ElevateHD\"},\"itsounds\":{\"heading\":\"itSOUNDS\",\"intro\":\"With itSOUNDS, our task was to bring the company’s online presence in line with its brand identity. Our web redesign not only matched the brand’s high-end nature, but was able to evoke the mysterious charm of listening to music with considered animations and interaction tweaks.\",\"label\":\"Premium design speakers\"},\"meatandbones\":{\"heading\":\"Meat & Bones\",\"intro\":\"Meat & Bones has a special place in our hearts. We’ve been there since\\u0003 its inception, figuring out whether there is enough demand for such a service, and made it grow into the service it is today, using our design, development, and marketing expertise to create something delightful.\",\"label\":\"Dog food service\",\"link\":\"Explore Meat & Bones\"},\"ppcbee\":{\"heading\":\"PPCBee\",\"intro\":\"PPC Bee is a tool for the automation of pay-per-click product campaigns for online stores. Started outside Blueberry as a set of scripts, we grew it into a capable system with a strong team behind it and a user-centric focus.\",\"label\":\"PPC tool for online stores\"},\"zaplo\":{\"heading\":\"Zaplo\",\"intro\":\"With the Zaplo.dk project, we were tasked with figuring out how to make software releases faster for 4finance, a leading global digital consumer finance company. We not only showed how work could be done more \\u0003efficiently, but also how to communicate effectively and structure code better, setting an example for their other projects to follow.\",\"label\":\"Global consumer finance\",\"link\":\"Explore Zaplo.dk\"}},\"colaborate\":\"Lets start working together\",\"detail\":{\"elevate\":{\"heading\":\"elevateHD\",\"client\":{\"app\":\"iOS app\",\"rails\":\"Ruby on Rails\",\"angular\":\"AngularJS\",\"heading\":\"About\\u0003 the Client\",\"subheading\":\"Collaboration\",\"ux\":\"UX design\",\"contentful\":\"Contentful CMS\",\"chef\":\"Chef\",\"es6\":\"ECMAScript 2015\",\"text\":\"elevateHD — Elevate Home Design allows users to quickly and cost-effectively design their own homes. It’s available on both iOS and the web. The service is offered to the end-customer segment, but also exists as a licenced solution for B2B to improve online conversion rate and enhance the shopping experience for the customer.\",\"rubymotion\":\"RubyMotion\",\"translation\":\"Translation server\",\"frontend\":\"Web app front-end\",\"strategy\":\"Digital strategy\",\"backend\":\"Web app back-end\",\"nightwatch\":\"Nightwatch.js\",\"going\":\"2 years and \\u0003going\",\"design\":\"Visual design\",\"react\":\"React\"},\"live\":{\"link\":\"Visit elevateHD website\",\"text\":\"Check out live version\"},\"technology\":\"Technology Highlights\",\"about\":{\"heading\":\"The Challenge\",\"subheading\":\"Our Solution\",\"subtext1\":\"To test the client’s vision, we created a solid MVP iOS app. Results after testing showed a need for a restructuring of the business concept, and our cooperation with the client shifted to a higher level.\",\"subtext2\":\"Our team helped shift focus from a B2C-only solution to both a B2C and B2B platform and brought the app to the web to reach a wider audience. Blueberry is now responsible for product management of the whole platform and drives innovation behind the product.\",\"text\":\"Our client came to us with a vision — a fast and affordable interior design \\u0003service without the inefficiencies of in-person consultations. The client asked us, the developers behind the project, to validate his business idea.\"},\"url\":\"http:\\u002F\\u002Fwww.elevatehomedesign.com\",\"perex\":\"elevateHD is a service that provides affordable home design to its users. While we were first called on only to create a minimum viable product for elevateHD, our collaboration grew deeper as we helped identify a viable business model, brought the product to the web, and ultimately became responsible for the entire product.\",\"hire\":\"Hire Us\",\"project\":{\"heading\":\"The Challenge\",\"subheading\":\"Our Solution\",\"subtext1\":\"To test the client’s vision, we created a solid MVP iOS app. Results after testing showed a need for a restructuring of the business concept, and our cooperation with the client shifted to a higher level.\",\"subtext2\":\"Our team helped shift focus from a B2C-only solution to both a B2C and B2B platform and brought the app to the web to reach a wider audience. Blueberry is now responsible for product management of the whole platform and drives innovation behind the product.\",\"text\":\"Our client came to us with a vision — a fast and affordable interior design \\u0003service without the inefficiencies of in-person consultations. The client asked us, the developers behind the project, to validate his business idea.\"},\"link\":\"Visit elevateHD website\"},\"meatandbones\":{\"heading\":\"Meat & Bones\",\"client\":{\"rails\":\"Ruby on Rails\",\"heading\":\"About\\u0003 the Client\",\"subheading\":\"Collaboration\",\"ux\":\"UX design\",\"marketing\":\"Marketing strategy\",\"stripe\":\"Stripe\",\"text\":\"Meat & Bones provides raw dog food as a subscription service. This company has a special place in our hearts. We’ve been there since its inception, figuring out whether there is enough demand for such a service, and helped it grow into the service it is today, using our design, development, and marketing expertise to create a delightful, healthy, and easy way to feed pets.\",\"frontend\":\"Web app front-end\",\"strategy\":\"Digital strategy\",\"backend\":\"Web app back-end\",\"going\":\"1 years and\\u0003 going\",\"design\":\"Visual design\",\"react\":\"React\"},\"live\":{\"link\":\"Visit Meat & Bones website\",\"text\":\"Check out live version\"},\"technology\":\"Technology Highlights\",\"url\":\"http:\\u002F\\u002Fwww.meatandbones.com\",\"perex\":\"Meat & Bones provides raw dog food as a subscription service. We worked with the company during inception to determine whether or not there was enough demand for such a service. We helped Meat&Bones grow into the service it is today using our design, development, and marketing expertise to create a healthy and easy way to feed pets.\",\"hire\":\"Hire Us\",\"project\":{\"heading\":\"The Challenge\",\"subheading\":\"Our Solution\",\"subtext1\":\"A multidisciplinary team was assigned to the project. In the first phase, we created a landing page and an ad campaign, the goal of which was to gauge the general interest of customers\",\"subtext2\":\"After confirming interest, we began the service development process. We researched competitors and interviewed potential customers as a basis, and furthered this with mockups and the release of an MVP. Our first version, which only allowed pre-orders, was used to test and promote the service as well as to gain data for further iteration\",\"subtext3\":\"Besides developing the platform, we’re marketing the service and creating new campaigns while we flesh out the website. We’re helping Meat & Bones create a great service for dog-lovers who care about proper nutrition and a healthy life\",\"text\":\"The client came to us to gain insight into customer interest for a raw dog food subscription service and asked us to build an online platform focused on the presentation and the delivery of the food.\"},\"link\":\"Visit Meat & Bones website\"},\"tcox\":{\"client\":{\"heading\":\"About\\u0003 the Client\",\"subheading\":\"Collaboration\",\"goingText\":\"We know our stuff. Our devs give advanced workshops on the technologies we work with.\",\"developmentText\":\"We know our stuff. Our devs give advanced workshops on the technologies we work with.\",\"agileText\":\"We know our stuff. Our devs give advanced workshops on the technologies we work with.\",\"text\":\"Tayllor & Cox is an Accredited Training Organisation for PRINCE 2, MOP, \\u0003ITIL, SCRUM, MSP, ISMS and more accredited by Axelos. At the same time, \\u0003it works as a Registered Certification Body for professional services, audits and career development. It is eminent in the Czech Republic, and all around Europe.\",\"going\":\"4 months of development\",\"development\":\"Change development\",\"agile\":\"Agile development\"},\"heading\":\"Tayllor & Cox\",\"hire\":\"Hire Us\",\"link\":\"Visit TAYLLORCOX website\",\"live\":{\"link\":\"Visit TAYLLORCOX website\",\"text\":\"Check out live version\"},\"perex\":\"Tayllor & Cox is a training and certification organisation. The complete redesign of the Tayllor & Cox website was a thorough yet rewarding task for us. We’re quite proud of the meticulously optimized buying process and the more effective homepage, but also of the custom-made CMS we created in the process, making it easy and efficient to administer the complex website.\",\"project\":{\"heading\":\"The Challenge\",\"subheading\":\"Our Solution\",\"subtext1\":\"The project started with analysis of the website, especially focused on the buying process and its weak spots. We also gathered information about customer profiles and did competitor research. After this phase, we started with prototyping the website with respect to key findings about the needs of customers. Findings were reflected in the homepage design to bring important touchpoints to the front. and in the right order.\",\"subtext2\":\"Special care was taken with regards to the buying process, where we got through many rounds of iteration and user testing. Innovated backend includes custom made CMS with improved UX and functionality enabling anybody to work with it, ensuring consistency of content design on the website. The backend is connected to and automated email templates system. In general, we have delivered an innovative website with improved usability and higher online conversion potential and the backend easy to maintain, with automated buying process features.\",\"text\":\"Tayllor & Cox set a clear goal to create a number one website, representing its dominance and excellence in the market. The client faced many challenges with its robust and complex web presentation. There was no mobile optimization, it was difficult and time consuming to load web content, there was a low rate of online conversions, and the site did not reflect the profile of the most frequent customers enough. Above all, the client wanted to improve the level of automation in the buying process online - i.e. reduce assistance needed by customer support in the order form process.\"}},\"zaplo\":{\"heading\":\"Zaplo.dk\",\"client\":{\"head\":\"The Client\",\"heading\":\"About the Client\",\"subheading\":\"Collaboration\",\"contentful\":\"Contentful CMS\",\"chef\":\"Chef\",\"es6\":\"ECMAScript 2015\",\"text\":\"4Finance is a global leader in digital consumer finance, using cutting-edge technology and data to offer fast and convenient loans to customers around the world.\",\"translation\":\"Translation server\",\"frontend\":\"Web app front-end\",\"strategy\":\"Digital strategy\",\"nightwatch\":\"Nightwatch.js\",\"going\":\"4 months\",\"development\":\"Change development\",\"react\":\"React\"},\"live\":{\"link\":\"Visit Zaplo.dk website\",\"text\":\"Check out live version\"},\"technology\":\"Technology Highlights\",\"url\":\"http:\\u002F\\u002Fwww.zaplo.dk\",\"perex\":\"Zaplo.dk is a web application we made for 4finance, a leading consumer finance company. It allows people to get a loan quickly. In creating it, we not only showed how work could be completed more efficiently, but also how to communicate effectively and structure code better, setting an example for other 4finance projects to follow.\",\"hire\":\"Hire Us\",\"project\":{\"heading\":\"The Challenge\",\"subheading\":\"Our Solution\",\"subtext1\":\"To speed up front-end development, we worked alongside the 4finance team to deeply understand the problem, the product, and the people. This investigation lead us to reorganize the multidisciplinary development teams, which improved the communication between back-end and front-end developers, thus reducing the time needed to complete the process. Taking into account the need for more skilled developers, we researched and selected React.js as the best platform. Besides bringing in more development power, React.js allowed us to reuse more code, significantly reducing development time. \",\"subtext2\":\"The project was a resounding success. Zaplo.dk demonstrated how work could be completed more efficiently, created better communication, and improved code structure — setting a precedent for their future projects.\",\"text\":\"The client came to us to solve a recurring problem: the development process behind software releases was taking too long. We were tasked with developing a solution as a proof-of-concept of our new approach. We conducted an in-depth analysis of existing 4finance projects and concluded that two main factors were to blame: it was taking too long to make the front-end for websites and it was hard to find developers experienced in the technological framework that was being used.\",\"text1_1\":\"The client came to us to solve a recurring problem: the development process behind software releases was taking too long. We were tasked with developing a solution as a proof-of-concept of our new approach.\",\"text2_1\":\"To speed up front-end development, we worked alongside the 4finance team to deeply understand the problem, the product, and the people. This investigation lead us to reorganize the multidisciplinary development teams, which improved the communication between back-end and front-end developers, thus reducing the time needed to complete the process.\",\"text1_2\":\"We conducted an in-depth analysis of existing 4finance projects and concluded that two main factors were to blame: it was taking too long to make the front-end for websites and it was hard to find developers experienced in the technological framework that was being used.\",\"text2_2\":\"Taking into account the need for more skilled developers, we researched and selected React.js as the best platform. Besides bringing in more development power, React.js allowed us to reuse more code, significantly reducing development time.\",\"text2_3\":\"The project was a resounding success. Zaplo.dk demonstrated how work could be completed more efficiently, created better communication, and improved code structure — setting a precedent for their future projects.\"},\"link\":\"Visit Zaplo.dk website\"}},\"heading\":\"Our Work\",\"more\":\"Show all our projects\"},\"hello\":\"Hieerlosasdasasssssssasdasda\",\"twitter\":{\"heading\":\"Life in our team\",\"loading\":\"Loading Blueberry tweets\",\"provider\":\"via Twitter\"},\"footer\":{\"copyright\":{\"by\":\"This page was handcrafted with\",\"company\":\"© 2016 Blueberry.io\",\"where\":\"in United States\"},\"section\":{\"company\":{\"heading\":\"Company\",\"items\":{\"contacts\":{\"heading\":\"Contacts\"},\"team\":{\"heading\":\"Our team\"},\"work\":{\"heading\":\"Our work\"}}},\"contact\":{\"heading\":\"Contact us\",\"items\":{\"cz\":{\"heading\":\"Křížová 2598\\u002F4\\u003Cbr \\u002F\\u003EPrague, Czech Republic\"},\"srb\":{\"heading\":\"Nikole Pašića 29\\u003Cbr\\u002F\\u003ENovi Sad, Serbia\"},\"us\":{\"heading\":\"We Work\\u003Cbr\\u002F\\u003E220 NW 8th Ave\\u003Cbr\\u002F\\u003EPortland, OR 97205\"}}},\"social\":{\"heading\":\"Social\",\"items\":{\"facebook\":{\"heading\":\"Facebook\"},\"linkedIn\":{\"heading\":\"LinkedIn\"},\"twitter\":{\"heading\":\"Twitter\"}}}}},\"team\":{\"group\":{\"applications\":\"Applications\",\"bussiness_development\":\"Business Development\",\"design\":\"Design\",\"development\":\"Development\",\"hr_operations\":\"HR & Operations\",\"leadership\":\"Leadership\",\"marketing\":\"Marketing\",\"webdesign\":\"Webs and design\"},\"heading\":\"Our team\",\"heading2\":\"True perfection lies in people you work with\",\"heading3\":\"We create stunning web sites and fresh design\",\"heading4\":\"Want to be part of our team?\"},\"locations\":{\"heading\":\"Where we work\",\"office\":\"Our office in\",\"prg\":{\"address\":\"Křížová 4, Praha 5\\u003Cbr \\u002F\\u003E150 00\",\"city\":\"Prague, Czech Republic\",\"heading\":\"Prague\",\"street\":\"Křížová 4, Praha 5\",\"zip\":\"150 00\"},\"sf\":{\"address\":\"220 NW 8th Ave\\u003Cbr \\u002F\\u003EPortland, OR 97205\",\"city\":\"Oregon, USA\",\"heading\":\"Portland\",\"street\":\"220 NW 8th Ave\",\"zip\":\"Portland, OR 97205\"},\"srb\":{\"city\":\"Novi Sad, Serbia\",\"heading\":\"Novi Sad\",\"street\":\"Nikole Pašića 29\",\"zip\":\"21000\"}},\"menu\":{\"contact\":\"Contact\",\"home\":\"Home\",\"team\":\"Team\",\"work\":\"Work\"},\"workflow\":{\"agile\":{\"heading\":\"Agile Development\",\"text1\":\"We follow the Scrum agile development methodology. We work in short iterations called sprints. After each sprint, we deliver a working product with new features and present it to you.\",\"text2\":\"You are always in control of the development process and can make changes to the product as we go, and ultimately decide when the right time to launch is. While we build, we gather feedback from  customers and make sure that we\'re developing the right product.\"},\"brief\":{\"heading\":\"Brief\",\"text\":\"We are open to discussing your business. The first meeting helps both sides understand each other and set the right expectations.\"},\"heading\":\"We are proud of how we work\",\"idea\":{\"heading\":\"Ideation & Exploration\",\"text\":\"Based on different types of research, we approach design challenges with the whole team from multiple perspectives. This helps us map out user \\u0003behavior and understand it.\"},\"prototype\":{\"heading\":\"Prototype & Visual Design\",\"text\":\"We start to sketch with pen and paper to capture our initial thoughts. We then convert these wireframes into \\u0003a clickable prototype using various digital design tools. The prototype is used to test our ideas and assumptions in the real world. And finally, we prepare high fidelity visual design.\"},\"research\":{\"heading\":\"Research\",\"text\":\"No business plan survives first contact with customers. Understanding real people and their needs is at the core of what good design is built on. That is why basic assumptions need to be verified at the beginning. \\u0003However, collecting continuous feedback is key for \\u0003further improvement and allows us to respond flexibly to the users\' needs. We see research as a process rather than a stage.\"},\"strategy\":{\"heading\":\"Digital Strategy\\u0003 Consultation\",\"text\":\"Before we start designing your product, it is extremely \\u0003important for us to get familiar with your business idea. \\u0003Furthermore, we will help you analyze your business \\u0003environment and define your target audience. Based on the needs of your customers, we will propose an appropriate \\u0003digital strategy that will fit your business. Before we move on, we will test our assumptions.\"},\"ux\":{\"heading\":\"UX Design\",\"text\":\"User-centered design is our path to digital excellence. Combining research, creative ideas and prototyping, we design a product based on real user insights and with clear KPIs.\"}}}}},\"locale\":\"en\",\"fallbackLocale\":\"defaultEn\"}}</script><script src=\"app.xxx.js\" type=\"text/javascript\"></script></body></html>"`;
